CREATE DATABASE styintermed WITH TEMPLATE = template0 ENCODING = 'UTF8' LOCALE_PROVIDER = libc LOCALE = 'Portuguese_Brazil.1252';


ALTER DATABASE styintermed OWNER TO postgres;

\connect styintermed

SET statement_timeout = 0;
SET lock_timeout = 0;
SET idle_in_transaction_session_timeout = 0;
SET transaction_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SELECT pg_catalog.set_config('search_path', '', false);
SET check_function_bodies = false;
SET xmloption = content;
SET client_min_messages = warning;
SET row_security = off;

--
-- Name: pgcrypto; Type: EXTENSION; Schema: -; Owner: -
--

CREATE EXTENSION IF NOT EXISTS pgcrypto WITH SCHEMA public;


--
-- Name: EXTENSION pgcrypto; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION pgcrypto IS 'cryptographic functions';


SET default_tablespace = '';

SET default_table_access_method = heap;

--
-- Name: bling_tokens; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.bling_tokens (
    id integer NOT NULL,
    access_token text NOT NULL,
    refresh_token text NOT NULL,
    expires_in integer NOT NULL,
    created_at timestamp without time zone DEFAULT now() NOT NULL
);


ALTER TABLE public.bling_tokens OWNER TO postgres;

--
-- Name: bling_tokens_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.bling_tokens_id_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.bling_tokens_id_seq OWNER TO postgres;

--
-- Name: bling_tokens_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.bling_tokens_id_seq OWNED BY public.bling_tokens.id;


--
-- Name: cliente_b2b; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.cliente_b2b (
    idcliente integer NOT NULL,
    internocliente character varying(50) NOT NULL,
    cpfcnpj character varying(20) NOT NULL,
    nome character varying(1000) NOT NULL,
    email character varying(1000),
    ddd character varying(5),
    telefone character varying(20),
    statuscliente character varying(20)
);


ALTER TABLE public.cliente_b2b OWNER TO postgres;

--
-- Name: cliente_b2b_idcliente_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.cliente_b2b_idcliente_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.cliente_b2b_idcliente_seq OWNER TO postgres;

--
-- Name: cliente_b2b_idcliente_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.cliente_b2b_idcliente_seq OWNED BY public.cliente_b2b.idcliente;


--
-- Name: cliente_secundaria; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.cliente_secundaria (
    idclientesecundario integer NOT NULL,
    cpfcnpj character varying(20) NOT NULL,
    nome character varying(1000) NOT NULL,
    email character varying(1000),
    telefone character varying(20)
);


ALTER TABLE public.cliente_secundaria OWNER TO postgres;

--
-- Name: cliente_secundaria_idclientesecundario_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.cliente_secundaria_idclientesecundario_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.cliente_secundaria_idclientesecundario_seq OWNER TO postgres;

--
-- Name: cliente_secundaria_idclientesecundario_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.cliente_secundaria_idclientesecundario_seq OWNED BY public.cliente_secundaria.idclientesecundario;


--
-- Name: execution_logs; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.execution_logs (
    id integer NOT NULL,
    script_name character varying(255) NOT NULL,
    output text,
    created_at timestamp with time zone DEFAULT now()
);


ALTER TABLE public.execution_logs OWNER TO postgres;

--
-- Name: execution_logs_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.execution_logs_id_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.execution_logs_id_seq OWNER TO postgres;

--
-- Name: execution_logs_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.execution_logs_id_seq OWNED BY public.execution_logs.id;


--
-- Name: order_itens; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.order_itens (
    iditempedido integer NOT NULL,
    idpedido integer NOT NULL,
    barcode character varying(50),
    indexado character varying(1000) NOT NULL,
    quantidade integer NOT NULL,
    precounitario numeric(13,2) NOT NULL,
    subtotal numeric(13,2) NOT NULL
);


ALTER TABLE public.order_itens OWNER TO postgres;

--
-- Name: order_itens_iditempedido_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.order_itens_iditempedido_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.order_itens_iditempedido_seq OWNER TO postgres;

--
-- Name: order_itens_iditempedido_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.order_itens_iditempedido_seq OWNED BY public.order_itens.iditempedido;


--
-- Name: orders; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.orders (
    idpedido integer NOT NULL,
    numeropedido character varying(50) NOT NULL,
    nomecliente character varying(1000) NOT NULL,
    cpfcnpjcliente character varying(20) NOT NULL,
    tipopedido character varying(50) NOT NULL,
    statussincronismo boolean DEFAULT false NOT NULL,
    observacao character varying(5000),
    pedidosty integer,
    statusped character varying(50),
    liberado boolean DEFAULT false,
    created_at timestamp with time zone DEFAULT now() NOT NULL,
    email character varying,
    telefone character varying,
    estado character varying,
    transportadora character varying,
    pagamento character varying,
    parcelamento character varying,
    bandeira character varying,
    qtdpecas character varying,
    valorpedido money,
    pedidobling character varying,
    nfebling character varying,
    nfeid character varying,
    valornota money,
    valorfrete money
);


ALTER TABLE public.orders OWNER TO postgres;

--
-- Name: orders_idpedido_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.orders_idpedido_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.orders_idpedido_seq OWNER TO postgres;

--
-- Name: orders_idpedido_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.orders_idpedido_seq OWNED BY public.orders.idpedido;


--
-- Name: produtos; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.produtos (
    idproduto integer,
    referencia character varying(30) NOT NULL,
    sku character varying(30) NOT NULL,
    nome character varying(1000) NOT NULL,
    coresid character varying(1000),
    cores character varying(1000),
    grade character varying(1000),
    composicao character varying(1000),
    modolavar character varying(1000),
    ncm character varying(15),
    marca character varying(1000),
    colecao character varying(1000),
    largura numeric(10,3) DEFAULT 0.2,
    altura numeric(10,3) DEFAULT 0.2,
    profundidade numeric(10,3) DEFAULT 0.2,
    peso numeric(10,3),
    tituloso character varying(1000),
    descricaoso character varying(1000),
    descricaocurta character varying(1000),
    descricaolonga character varying(5000),
    palavraschave character varying(1000)
);


ALTER TABLE public.produtos OWNER TO postgres;

--
-- Name: sku_variacao_estoque; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.sku_variacao_estoque (
    indexado character varying(500),
    indexadocorescrito character varying(500),
    refcodigo character varying(50),
    variacao character varying(100),
    barcode character varying(15) NOT NULL,
    precob2c numeric(13,2),
    precob2b numeric(13,2),
    armazenamento integer,
    fisico integer,
    reserva integer,
    disponivel integer
);


ALTER TABLE public.sku_variacao_estoque OWNER TO postgres;

--
-- Name: sku_variacao_estoque2; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.sku_variacao_estoque2 (
    indexado character varying(500),
    indexadocorescrito character varying(500),
    refcodigo character varying(50),
    variacao character varying(100),
    barcode character varying(15) NOT NULL,
    precob2c numeric(13,2),
    precob2b numeric(13,2),
    armazenamento integer,
    fisico integer,
    reserva integer,
    disponivel integer
);


ALTER TABLE public.sku_variacao_estoque2 OWNER TO postgres;

--
-- Name: users; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.users (
    id integer NOT NULL,
    username character varying(255) NOT NULL,
    password_hash text NOT NULL,
    created_at timestamp without time zone DEFAULT now()
);


ALTER TABLE public.users OWNER TO postgres;

--
-- Name: users_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.users_id_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER SEQUENCE public.users_id_seq OWNER TO postgres;

--
-- Name: users_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.users_id_seq OWNED BY public.users.id;


--
-- Name: bling_tokens id; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.bling_tokens ALTER COLUMN id SET DEFAULT nextval('public.bling_tokens_id_seq'::regclass);


--
-- Name: cliente_b2b idcliente; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.cliente_b2b ALTER COLUMN idcliente SET DEFAULT nextval('public.cliente_b2b_idcliente_seq'::regclass);


--
-- Name: cliente_secundaria idclientesecundario; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.cliente_secundaria ALTER COLUMN idclientesecundario SET DEFAULT nextval('public.cliente_secundaria_idclientesecundario_seq'::regclass);


--
-- Name: execution_logs id; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.execution_logs ALTER COLUMN id SET DEFAULT nextval('public.execution_logs_id_seq'::regclass);


--
-- Name: order_itens iditempedido; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.order_itens ALTER COLUMN iditempedido SET DEFAULT nextval('public.order_itens_iditempedido_seq'::regclass);


--
-- Name: users id; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.users ALTER COLUMN id SET DEFAULT nextval('public.users_id_seq'::regclass);
